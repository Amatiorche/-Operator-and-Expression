using System;

class ThirdDigit
{
    static void Main()
    {
        string number = Console.ReadLine();
        int stringLength = number.Length;
        if (stringLength < 3)
        {
            Console.WriteLine("false 0");
        }
        else
        {
            int position = stringLength - 3;
            if (number[position] == '7')
            {
                Console.WriteLine("true");
            }
            else
            {
                Console.WriteLine("false " + number[position]);
            }
        }
    }
}
